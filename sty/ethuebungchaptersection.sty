    \PackageWarning{ethuebung}{**** Exercise sheet externally requested by %
      \protect\ethuebungwantexercisesheet: ignoring call to %
      \protect\UebungMakeSolutionsSheet}
  }%
  % 
  % Forbid making this a tips sheet.
  % 
